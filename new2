pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract XSTRM is ERC20 {

    address owner;

    constructor() ERC20("XSTRM", "X") { //  use owneable 
        owner = msg.sender ;
        _mint(owner, 10000); // add events 
    }
}
